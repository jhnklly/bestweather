{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "settings": {
        "react": {
            "pragma": "React"
        }
    },
    "ecmaFeatures": {
        "jsx": true
    },
    "rules": {
        "consistent-return": 0,
        "indent": [2, 4],
        "new-cap": 0,
        "no-param-reassign": [2, { "props": false }],
        "react/jsx-indent": [2, 4],
        "react/jsx-indent-props": [2, 4],
        "react/jsx-no-bind": 0,
        "no-underscore-dangle": 0,
        "eol-last": [2]
    }
}
